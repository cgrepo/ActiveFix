<h1>COMPUTADORAS</h1>


  <table>
  	<thead>
  		<th colspan="3">ID</th>
  		<th colspan="4">PC NAME</th>

  	</thead>
  	<tbody>
  		<% @pcs.each do |pc| %>
  			<td colspan="5"><%= pc.id %></td>
  			<td colspan="3"><%= pc.name %></td>
  			</tr>
  		<% end %>
  	</tbody>
  </table>

  <%= link_to 'NUEVA', new_pc_path %>
